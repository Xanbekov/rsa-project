CREATE\040TABLE\040Guests\040(
\040\040\040\040id\040INTEGER\040PRIMARY\040KEY\040AUTOINCREMENT,
\040\040\040\040first_name\040TEXT,
\040\040\040\040last_name\040TEXT,
\040\040\040\040email\040TEXT,
\040\040\040\040phone_number\040TEXT
);
CREATE\040TABLE\040Rooms\040(
\040\040\040\040id\040INTEGER\040PRIMARY\040KEY\040AUTOINCREMENT,
\040\040\040\040room_number\040TEXT,
\040\040\040\040type\040TEXT,
\040\040\040\040status\040TEXT
);
CREATE\040TABLE\040Bookings\040(
\040\040\040\040id\040INTEGER\040PRIMARY\040KEY\040AUTOINCREMENT,
\040\040\040\040guest_id\040INTEGER,
\040\040\040\040room_id\040INTEGER,
\040\040\040\040check_in_date\040TEXT,
\040\040\040\040check_out_date\040TEXT,
\040\040\040\040FOREIGN\040KEY\040(guest_id)\040REFERENCES\040Guests(id),
\040\040\040\040FOREIGN\040KEY\040(room_id)\040REFERENCES\040Rooms(id)
);
CREATE\040TABLE\040Employees\040(
\040\040\040\040id\040INTEGER\040PRIMARY\040KEY\040AUTOINCREMENT,
\040\040\040\040first_name\040TEXT,
\040\040\040\040last_name\040TEXT,
\040\040\040\040position\040TEXT
);
CREATE\040TABLE\040RoomService\040(
\040\040\040\040id\040INTEGER\040PRIMARY\040KEY\040AUTOINCREMENT,
\040\040\040\040room_id\040INTEGER,
\040\040\040\040service_type\040TEXT,
\040\040\040\040request_time\040TEXT,
\040\040\040\040FOREIGN\040KEY\040(room_id)\040REFERENCES\040Rooms(id)
);
.quit
