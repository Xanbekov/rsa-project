CREATE\040DATABASE\040hotel_db;
USE\040hotel_db;
CREATE\040TABLE\040users\040(
\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,
\040\040\040\040username\040VARCHAR(255)\040NOT\040NULL,
\040\040\040\040role\040VARCHAR(50)
);
CREATE\040TABLE\040rooms\040(
\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,
\040\040\040\040room_number\040VARCHAR(50)\040NOT\040NULL,
\040\040\040\040type\040VARCHAR(100),
\040\040\040\040status\040VARCHAR(50)
);
CREATE\040TABLE\040rooms\040(\040\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,\040\040\040\040\040room_number\040VARCHAR(50)\040NOT\040NULL,\040\040\040\040\040type\040VARCHAR(100),\040\040\040\040\040status\040VARCHAR(50)\040);
SHOW\040TABLES;
CREATE\040TABLE\040equipment\040(
\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,
\040\040\040\040name\040VARCHAR(255)\040NOT\040NULL,
\040\040\040\040type\040VARCHAR(100),
\040\040\040\040status\040VARCHAR(50),
\040\040\040\040location\040VARCHAR(255)
);
CREATE\040TABLE\040equipment\040(\040\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,\040\040\040\040\040name\040VARCHAR(255)\040NOT\040NULL,\040\040\040\040\040type\040VARCHAR(100),\040\040\040\040\040status\040VARCHAR(50),\040\040\040\040\040location\040VARCHAR(255)\040);
CREATE\040TABLE\040requests\040(
\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,
\040\040\040\040user_id\040INT\040NOT\040NULL,
\040\040\040\040equipment_id\040INT\040NOT\040NULL,
\040\040\040\040request_date\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP,
\040\040\040\040status\040VARCHAR(50),
\040\040\040\040description\040TEXT,
\040\040\040\040FOREIGN\040KEY\040(user_id)\040REFERENCES\040users(id),
\040\040\040\040FOREIGN\040KEY\040(equipment_id)\040REFERENCES\040equipment(id)
);
CREATE\040TABLE\040requests\040(\040\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,\040\040\040\040\040user_id\040INT\040NOT\040NULL,\040\040\040\040\040equipment_id\040INT\040NOT\040NULL,\040\040\040\040\040request_date\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP,\040\040\040\040\040status\040VARCHAR(50),\040\040\040\040\040description\040TEXT,\040\040\040\040\040FOREIGN\040KEY\040(user_id)\040REFERENCES\040users(id),\040\040\040\040\040FOREIGN\040KEY\040(equipment_id)\040REFERENCES\040equipment(id)\040);
CREATE\040TABLE\040logs\040(
\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,
\040\040\040\040user_id\040INT\040NOT\040NULL,
\040\040\040\040action\040VARCHAR(255)\040NOT\040NULL,
\040\040\040\040timestamp\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP,
\040\040\040\040FOREIGN\040KEY\040(user_id)\040REFERENCES\040users(id)
);
CREATE\040TABLE\040logs\040(\040\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,\040\040\040\040\040user_id\040INT\040NOT\040NULL,\040\040\040\040\040action\040VARCHAR(255)\040NOT\040NULL,\040\040\040\040\040timestamp\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP,\040\040\040\040\040FOREIGN\040KEY\040(user_id)\040REFERENCES\040users(id)\040);
SHOW\040TABLES;
